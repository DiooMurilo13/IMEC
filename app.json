      ["expo-document-picker"],
      [
        "expo-camera",
        {
          "cameraPermission": "Permita IMEC acessar sua camera."
        }
      ]
    ],
    "name": "IMEC",
    "slug": "IMEC",
    "version": "1.0.0",
    "orientation": "portrait",
