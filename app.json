      },
      "package": "com.ghostline.IMEC"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1431fa8c-1685-415d-8a01-c3cacac7a96c"
